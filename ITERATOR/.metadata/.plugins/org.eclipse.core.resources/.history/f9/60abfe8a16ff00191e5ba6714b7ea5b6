		bestSongs = new SongInfo[3];
		addSong("Roam", "B 52s", 1989);
		addSong("Cruel Summer", "Bananarama", 1984);
		addSong("Head Over Heels", "Tears For Fears", 1985);

	}


	@Override
	public Iterator<?> createIterator() {
		// TODO Auto-generated method stub
		return null;
	}

}
